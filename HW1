input1 = input("input1: ")
type1 = type(input1)
input2 = input("input2: ")
input3 = input("input3: ")
input4 = input("input4: ")
input5 = input("input5: ")
print("input1 :{} type: {}".format(input1, type1))
print("input2 :{} type: {}".format(input2, type1))
print("input3 :{} type: {}".format(input3, type1))
print("input4 :{} type: {}".format(input4, type1))
print("input5 :{} type: {}".format(input5, type1))
